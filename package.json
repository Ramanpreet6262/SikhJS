{
  "name": "sikhjs",
  "version": "0.0.3",
  "description": "Multiplatform application for Gurbani",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build:webpack": "webpack -p",
    "build:watch": "webpack -w",
    "start": "npm run build:webpack && open index.html",
    "package-darwin": "rm -rf SikhJS* && electron-packager . SikhJS --platform=darwin --arch=all --version=0.36.3 --ignore=\"(node_modules/babel*|docs/Documents)\"",
    "package-win32": "rm -rf SikhJS* && electron-packager . SikhJS --platform=win32 --arch=ia32 --version=0.36.3 --ignore=\"(node_modules/babel*|docs/Documents)\"",
    "package-linux": "rm -rf SikhJS* && electron-packager . SikhJS --platform=linux --arch=ia32 --version=0.36.3 --ignore=\"(node_modules/babel*|docs/Documents)\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "^3.3.6",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-remarkable": "^1.1.1",
    "react-router": "^2.0.0",
    "webpack": "^1.12.13"
  },
  "author": "bogas04",
  "license": "MIT"
}
